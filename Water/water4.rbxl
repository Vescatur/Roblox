						<ProtectedString name="Source">local water = script.Parent.Water
local grond = script.Parent.Ground
local diepte = script.Parent.Depth
local Sdiepte = script.Parent.SDepth
local getal1 = 0
local getal2 = 0


function ReSize()
	water.Size = Vector3.new (water.Size.x,(diepte.Value/10),water.Size.z)
	water.CFrame = CFrame.new (grond.CFrame.x,((grond.CFrame.y - 0.4)+(grond.Size.y/2)+(water.Size.y/2)),grond.CFrame.z)


	getal1 = 0
	getal2 = 0
	for i,v in pairs(script.Parent.Parent:children()) do		
		if v.Ground.Position.z &gt; grond.Position.z - 4.1 and v.Ground.Position.z &lt; grond.Position.z + 4.1 and v.Ground.Position.x &gt; grond.Position.x - 4.1 and v.Ground.Position.x &lt; grond.Position.x + 4.1 then
			print &quot;1&quot;
			getal1 = getal1 + v.Depth.Value
			getal2 = getal2 + 1
		end
	end

	getal1 = getal1 / getal2 

	for i,v in pairs(script.Parent.Parent:children()) do		
		if v.Ground.Position.z &gt; grond.Position.z - 4.1 and v.Ground.Position.z &lt; grond.Position.z + 4.1 and v.Ground.Position.x &gt; grond.Position.x - 4.1 and v.Ground.Position.x &lt; grond.Position.x + 4.1 then
			v.Depth.Value = getal1
			if v.Depth.Value &lt; 0 then 
				v.Depth.Value = 0
				v.Water.Size = Vector3.new (v.Water.Size.x,(v.Depth.Value/10),v.Water.Size.z)
				v.Water.CFrame = CFrame.new (v.Ground.CFrame.x,((Ground.CFrame.y - 0.4)+(Ground.Size.y/2)+(v.Water.Size.y/2)),Ground.CFrame.z)
			end

		end
	end

	water.Size = Vector3.new (water.Size.x,(diepte.Value/10),water.Size.z)
	water.CFrame = CFrame.new (grond.CFrame.x,((grond.CFrame.y - 0.4)+(grond.Size.y/2)+(water.Size.y/2)),grond.CFrame.z)

end

ReSize()


while true do
	if Sdiepte.Value  == diepte.Value  then
	else
		ReSize()
		Sdiepte.Value = script.Parent.Depth.Value
	end	
	
	wait(0.5 + (math.random(-500,500)/100) )
end






