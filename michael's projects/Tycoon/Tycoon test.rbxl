						<ProtectedString name="Source">local gui = script.Parent
local on = gui.On
local type = gui.Type

on.Changed:connect(function()
if on.Value == true then
On()
end
end)

function Do()
if type.Value == &quot;Alert&quot; then
for i = 1, 3 do
wait(.5)
gui.BackgroundColor = BrickColor.new(&quot;Really red&quot;)
wait(.5)
gui.BackgroundColor = BrickColor.new(&quot;Navy blue&quot;)
end
Off()
else
wait(3)
Off()
end
end

function On()
gui.Visible = true
for i = -1, 0, 0.1 do
wait()
gui.Position = UDim2.new(i, 0, 0.1, 0)
end
for i2 = 0.8, 0, -0.05 do
wait()
gui.Transparency = i2
end
Do()
end

function Off()
for i = 0, 0.8, 0.05 do
wait()
gui.Transparency = i
end
for i2 = 0, -1, -0.1 do
wait()
gui.Position = UDim2.new(i2, 0, 0.1, 0)
end
gui.Visible = false
on.Value = false
end </ProtectedString>
