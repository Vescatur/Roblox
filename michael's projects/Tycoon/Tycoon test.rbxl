								<ProtectedString name="Source">local tycoon = script.Parent.Parent.Parent
local owner = tycoon.Owner
local debounce = false
local cost = 50 -- change 50 to the price you want the upgrade to cost
local upgrade = tycoon:findFirstChild(&quot;Example&quot;) -- put the EXACT name of the upgrade
local newUpgrade = upgrade:Clone()
wait(.1)
upgrade:Remove()

script.Parent.Touched:connect(function(hit)
local player = game.Players:GetPlayerFromCharacter(hit.Parent)
if player ~= nil and debounce == false then
local alert = player.PlayerGui.TycoonGui.Alert
debounce = true
if owner.Value ~= player.Name then
alert.Text = &quot;You are not the owner of this tycoon!&quot; -- you can change &quot;tycoon&quot; to whatever you want
alert.Type.Value = &quot;Alert&quot; -- don&apos;t mess with this
alert.On.Value = true
wait(2) -- you can change the wait time to allow to be touched again
debounce = false
return end
if player.leaderstats.Money.Value &lt; cost then
local mn = cost-player.leaderstats.Money.Value
alert.Text = &quot;Insufficient funds! You need $&quot; ..mn.. &quot; more!&quot;
alert.Type.Value = &quot;Alert&quot;
alert.On.Value = true
wait(2) -- you can change the wait time to allow to be touched again
debounce = false
return end
player.leaderstats.Money.Value = player.leaderstats.Money.Value - cost
newUpgrade.Parent = tycoon
alert.Text = &quot;Upgrade complete!&quot; -- you can change what it says here. For an example, you&apos;re buying a chair. Chair bought!
alert.Type.Value = &quot;Message&quot; -- don&apos;t mess with this
alert.On.Value = true
for i = 0.4, 1, 0.01 do
wait()
script.Parent.Transparency = i
end
script.Parent.Parent:Remove()
end
wait(2) -- you can change the wait time to allow to be touched again
debounce = false
end)</ProtectedString>
