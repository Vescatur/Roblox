function shuffleWords()
	local newOrder = {}
	while #words &gt; 0 do
		local nextChoice = math.random(#words)
		table.insert(newOrder, words[nextChoice])
		table.remove(words, nextChoice)
	end
	words = newOrder
end

function searchTable(t, value)
	if t == nil or value == nil then return end
	for i=1,#t do
		if type(value) == &quot;table&quot; then
			if searchTable(value, t[i]) then return true end
		elseif type(value) == &quot;string&quot; and type(t[i]) == &quot;string&quot; then
			if string.lower(t[i]) == string.lower(value) then
				return true
			end
		elseif t[i] == value then
			return true
		end
	end
	return false
end

function randomViewingPosition()
	return randomBrickPos(choose({game.Workspace.GFloor, game.Workspace.GFloor2, game.Workspace.GFloor3}))
end

function randomBrickPos(brick) -- Returns a random position on top of a brick. It might error out if the selected brick is too small.
	local border = 5 -- The thickness of the border arround the outside of the brick that will not be chosen.
	if brick == nil or (brick.className ~= &quot;Part&quot; and brick.className ~= &quot;SpawnLocation&quot;) then
		print(&quot;Error! Invalid input for function randomBrickPos&quot;)
		return Vector3.new(0, 0, 0)
	end
	local xPos = math.random(brick.Position.x - ((brick.Size.x - border) / 2), brick.Position.x + ((brick.Size.x - border) / 2))
	local yPos = brick.Position.y + (brick.Size.y / 2) + 3 -- Directly Above the Brick
	local zPos = math.random(brick.Position.z - ((brick.Size.z - border) / 2), brick.Position.z + ((brick.Size.z - border) / 2))

	return Vector3.new(xPos, yPos, zPos)
end

