			</Properties>
		</Item>
		<Item class="Script" referent="RBX25">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">VoteFix</string>
				<ProtectedString name="Source">-- Ajedi32

voted = {}

gScript = game.Workspace.newGameScript

function hasVoted(player)
	if player == nil or player.Parent == nil then return true end
	for i=1,#voted do
		if voted[i] == player.Name then
			return true
		end
	end
	return false
end

function recountVotes()
	for i=1,#voted do
		if game.Players:findFirstChild(voted[i]) == nil then
			table.remove(voted, i)
			recountVotes()
			return true
		end
	end
	return false
end

function onChatted(msg, recipient, speaker)
	msg = string.lower(msg)
	if msg == &quot;vote reset&quot; or msg == &quot;reset&quot; or msg == &quot;reset/&quot; or msg == &quot;fix&quot; or msg == &quot;fix/&quot; then
		if not hasVoted(speaker) then
			table.insert(voted, speaker.Name)
			recountVotes()
			if #voted &gt;= (game.Players.NumPlayers * 0.75) then
				local m = Instance.new(&quot;Message&quot;)
				m.Name = &quot;Fixing&quot;
				m.Text = &quot;Fixing Game...&quot;
				m.Parent = game.Workspace
				voted = {}
				gScript.Parent = nil
				wait(0.1)
				gScript.Parent = game.Workspace
				wait(2)
				m.Parent = nil
			end
		end
	end
end

function onPlayerEntered(newPlayer)
	newPlayer.Chatted:connect(function(msg, recipient) onChatted(msg, recipient, newPlayer) end) 
end

game.Players.ChildAdded:connect(onPlayerEntered)</ProtectedString>
