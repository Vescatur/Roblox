					<string name="Name">LocalScript</string>
					<ProtectedString name="Source">-- Ajedi32

bin = script.Parent
tool = bin
LocalPlayer = tool.Parent.Parent

testmode = false -- Whether to allow bricks to be drawn on objects other than the board for testing

drawnBricks = {}

mouseDown = false
inPalette = false
startDrag = nil

drawColor = tool.Color
selectedColor = nil
mode = &quot;Draw&quot;
brushShape = &quot;Circle&quot;
brushSize = 2
lineSize = 2

drawBrick = game.Lighting.drawBrick

guiCreated = tool.guiCreated

---- Assign Variables for the main parts of the GUI
guiMain = bin.ColorToolGui:clone() -- GuiMain
	paletteFrame = guiMain.palette
	message = guiMain.Message
	toolbar = guiMain.Toolbar -- Toolbar
		optionsMenu = toolbar.Options -- Options
			brushOptions = optionsMenu.Brush
			lineOptions = optionsMenu.Line
		toolsMenu = toolbar.Toolset
		colorSelector = toolbar.ColorSelector
----

messageDebounce = 0
function displayMessage(m)
	messageDebounce = messageDebounce + 1
	message.Text = m
	message.Visible = true
	wait(3)
	messageDebounce = messageDebounce - 1
	if messageDebounce == 0 then
		message.Visible = false
	end
end

function clearNilBricks()
	local bricks = {}
	for i=1,#drawnBricks do
		if drawnBricks[i] ~= nil and drawnBricks[i].Parent ~= nil then
			table.insert(bricks, drawnBricks[i])
		end
	end
	drawnBricks = bricks
end

function drawInk(mouse)
	local pos
	local targ
	if mouse ~= nil then
		pos = mouse.Hit.p + Vector3.new(0, 0, 0.1)
		targ = mouse.target
	end
	local drawModel = game.Workspace:findFirstChild(&quot;drawing&quot;)
	if drawModel == nil then
		drawModel = Instance.new(&quot;Model&quot;)
		drawModel.Name = &quot;drawing&quot;
		drawModel.Parent = game.Workspace
	end

	if targ ~= nil and targ.Name == drawBrick.Name then
		targ.BrickColor = drawColor.Value
	else
		local newDraw = drawBrick:clone()
		newDraw.BrickColor = drawColor.Value
		newDraw.Position = pos
		newDraw.Mesh.Scale = Vector3.new(brushSize + 0.5, brushSize + 0.5, 0.1)
		if brushShape == &quot;Square&quot; then
			newDraw.Mesh.MeshType = &quot;Brick&quot;
		elseif brushShape == &quot;Circle&quot; then
			newDraw.Mesh.MeshType = &quot;Sphere&quot;
		end
		newDraw.Parent = drawModel
		table.insert(drawnBricks, newDraw)
	end
end

lineBrick = nil
function startLineDraw(mouse)
	local pos
	local targ
	if mouse ~= nil then
		pos = mouse.Hit.p + Vector3.new(0, 0, 0.1)
		targ = mouse.target
	end
	local drawModel = game.Workspace:findFirstChild(&quot;drawing&quot;)
	if drawModel == nil then
		drawModel = Instance.new(&quot;Model&quot;)
		drawModel.Name = &quot;drawing&quot;
		drawModel.Parent = game.Workspace
	end

	if targ ~= nil then
		startDrag = mouse.Hit.p + Vector3.new(0, 0, 0.1)
		lineBrick = Instance.new(&quot;Part&quot;)
		lineBrick.Name = &quot;drawLine&quot;
		lineBrick.formFactor = 0 
		lineBrick.Size = Vector3.new(1,1,1)
		lineBrick.BrickColor = drawColor.Value
		lineBrick.Anchored = true 
		lineBrick.Locked = true 
		lineBrick.CanCollide = false 

		local m = Instance.new(&quot;BlockMesh&quot;)
		m.Scale = Vector3.new(0.1, lineSize, 1)
		m.Parent = lineBrick

		lineBrick.Parent = drawModel
		table.insert(drawnBricks, lineBrick)
	end
end

function dragLineDraw(mouse)
	if lineBrick ~= nil and mouse ~= nil then
		local oldParent = lineBrick.Parent
		lineBrick.Parent = nil
		local point1 = startDrag
		local point2 = mouse.Hit.p + Vector3.new(0, 0, 0.1)
		local Distance = (point1- point2).magnitude
		lineBrick.Size = Vector3.new(1, 1, Distance)
		if lineBrick.Size.z ~= Distance then
			lineBrick.Mesh.Scale = Vector3.new(0.1, lineSize, 1 + (Distance - lineBrick.Size.z)/lineBrick.Size.z)
		end
		lineBrick.CFrame = CFrame.new(point1, point2)*CFrame.new(0, 0, -Distance/2)
		lineBrick.Parent = oldParent
	end
end

-- Color Palette Functions
function onMouseLeaveColor()
	if oldHoverSelection ~= nil then
		oldHoverSelection.Parent.BackgroundColor = BrickColor.Black()
		oldHoverSelection = nil
	end
end

function onMouseHoverColor(guiButton)
	onMouseLeaveColor()
	if guiButton ~= selectedButton then
		guiButton.Parent.BackgroundColor = BrickColor.White()
		oldHoverSelection = guiButton
	end
end

function onMouseSelectColor(guiButton)
	if selectedColor ~= nil then
		selectedColor.Parent.BackgroundColor = BrickColor.Black()
	end
	guiButton.Parent.BackgroundColor = BrickColor.Yellow()
	colorSelector.BackgroundColor = guiButton.BackgroundColor
	drawColor.Value = guiButton.BackgroundColor
	selectedColor = guiButton
	
	onMouseLeavePalette(paletteFrame)
	
	print(&quot;Selected Color: &quot; .. guiButton.BackgroundColor.Name)
end

function onMouseEnterPalette(mouse)
	mouse.Icon =&quot;rbxasset://textures\\ArrowCursor.png&quot;
	inPalette = true
end
function onMouseLeavePalette(mouse)
	paletteFrame.Visible = false
	inPalette = false
end

function buildPalette(mouse)
	paletteFrame.MouseEnter:connect(function() print(&quot;EnterPalette&quot;) onMouseEnterPalette(mouse) end)
	paletteFrame.MouseLeave:connect(function() print(&quot;LeavePalette&quot;)  onMouseLeavePalette(mouse) end)

	colorSelector.MouseButton1Down:connect(function() print(&quot;Showdialog&quot;) paletteFrame.Visible = true end)

--[[	local hoverSelection = Instance.new(&quot;TextLabel&quot;)
	hoverSelection.Position = UDim2.new(0.0, 0, 0.0, 0)
	hoverSelection.Size = UDim2.new(0.75, 0, 1.0, 0)
	hoverSelection.Text = &quot;&quot;
	hoverSelection.BackgroundColor = BrickColor.Black()
	hoverSelection.TextColor = BrickColor.White()
	hoverSelection.Text = primaryColor.BackgroundColor.Name
	hoverSelection.Parent = sideBar]]

	for xOffset = 0, 7 do
		for yOffset = 0,7 do
			local color = BrickColor.palette(xOffset + yOffset*8)

			local guiFrame = Instance.new(&quot;Frame&quot;)
			guiFrame.Position = UDim2.new(1.0/8 * xOffset, 0, 1.0/8*yOffset, 0)
			guiFrame.Size = UDim2.new(1.0/8, 0, 1.0/8, 0)
			guiFrame.BackgroundColor = BrickColor.Black()
			guiFrame.BorderSizePixel = 0
			guiFrame.Name = color.Name
			guiFrame.Parent = paletteFrame
		
			local guiButton = Instance.new(&quot;TextButton&quot;)
			guiButton.Position = UDim2.new(0.0, 2, 0.0, 2)
			guiButton.Size = UDim2.new(1.0, -4, 1.0, -4)
			guiButton.Text = &quot;&quot;
			guiButton.BorderSizePixel = 0
			guiButton.AutoButtonColor = false
			guiButton.BackgroundColor = color
			guiButton.MouseEnter:connect(function() onMouseHoverColor(guiButton) end)
			guiButton.MouseButton1Up:connect(function() onMouseSelectColor(guiButton) end)
			--guiButton.MouseButton2Down:connect(function() onMouseClick(secondaryColor, guiButton) end)
			guiButton.Parent = guiFrame

			if guiButton.BackgroundColor == colorSelector.BackgroundColor then
				guiFrame.BackgroundColor = BrickColor.White()
				selectedColor = guiButton
			end
		end
	end
end

function toolSelected(name)
	lineBrick = nil
	if name == &quot;Text&quot; then
		displayMessage(&quot;Come on! You know the rules: No writing the words out! =D&quot;)
		return
	end
	if toolsMenu:findFirstChild(name) ~= nil and toolsMenu[name].Active == false then
		displayMessage(&quot;This feature has not been created yet, but it may come out sometime in the future.&quot;)
		return
	end

	for n,tool in pairs(toolsMenu:getChildren()) do
		if tool.Name ~= name then
			tool.BackgroundColor3 = Color3.new(1, 1, 1)
		else
			tool.BackgroundColor3 = Color3.new(0, 0, 1)
			mode = name
		end
	end

	if mode == &quot;Draw&quot; then
		brushOptions.Visible = true
		lineOptions.Visible = false
	elseif mode == &quot;Line&quot; then
		brushOptions.Visible = false
		lineOptions.Visible = true
	else
		brushOptions.Visible = false
		lineOptions.Visible = false		
	end

	if mode == &quot;Eraser&quot; then
		displayMessage(&quot;Tip: At any time while you are drawing, you can press Esc to clear the drawing board.&quot;)
	end
end

function brushSelected(brush)
	local brushes = optionsMenu.Brush.Circle:getChildren()
	for i,b in pairs(optionsMenu.Brush.Square:getChildren()) do
		table.insert(brushes, b)
	end

	for i,b in pairs(brushes) do
		if b ~= brush then
			b.BackgroundColor3 = Color3.new(0.7, 0.7, 0.7)
		else
			b.BackgroundColor3 = Color3.new(0, 0, 1)
			brushShape = brush.Parent.Name
			brushSize = tonumber(string.sub(brush.Name, 5))
		end
	end
end

function lineSizeSelected(size)
	for i,s in pairs(lineOptions:getChildren()) do
		if s ~= size then
			s.BackgroundColor3 = Color3.new(0.7, 0.7, 0.7)
		else
			s.BackgroundColor3 = Color3.new(0, 0, 1)
			lineSize = tonumber(string.sub(size.Name, 5))
		end
	end
end

function partOwned(part)
	for i,c in pairs(drawnBricks) do
		if c == part then return true end
	end
	return false
end

function mouseClick3d(mouse)
	mouseDown = true
	local targ = mouse.Target
	if targ ~= nil then
		if mode == &quot;Line&quot; and (targ.Name == &quot;Board&quot; or ((targ.Name == &quot;drawLine&quot; or targ.Name == &quot;drawBrick&quot;) and partOwned(targ)) or testmode) then
			startLineDraw(mouse)
		end
	end
	mouseMove3d(mouse)
end

function mouseUnclick3d(mouse)
	mouseDown = false
	lineBrick = nil
end

function mouseMove3d(mouse)
	local targ = mouse.Target
	if targ ~= nil and mouseDown then
		if mode == &quot;Draw&quot; then
			if targ.Name == &quot;Board&quot; or testmode then
				drawInk(mouse)
			end
		elseif mode == &quot;Line&quot; and lineBrick ~= nil then
			if targ.Name == &quot;Board&quot; or ((targ.Name == &quot;drawLine&quot; or targ.Name == &quot;drawBrick&quot;) and partOwned(targ)) or testmode then
				dragLineDraw(mouse)
			end
		elseif mode == &quot;Eraser&quot; then
			if (targ.Name == &quot;drawLine&quot; or targ.Name == &quot;drawBrick&quot;) and partOwned(targ) then
				mouse.Target:remove()
			end
		end
	end
end

function keyDown(key)
	if key == string.char(27) then -- Esc
		for i=1,#drawnBricks do
			if drawnBricks[i] ~= nil and drawnBricks[i].Parent ~= nil then
				drawnBricks[i]:remove()
			end
		end
		drawnBricks = {}
	end
end

function onEquippedLocal(mouse)
	if not guiCreated.Value then
		guiMain.Parent = LocalPlayer.PlayerGui
		buildPalette(mouse)
		guiCreated.Value = true
	end
	toolbar.Visible = true
	mouseConnection1 = mouse.Button1Down:connect(function () mouseClick3d(mouse) end)
	mouseConnection2 = mouse.Button1Up:connect(function () mouseUnclick3d(mouse) end)
	moveConnection = mouse.Move:connect(function () mouseMove3d(mouse) end)
	keyConnection = mouse.KeyDown:connect(keyDown)
end

function onUnequippedLocal()
	if mouseConnection1 ~= nil then
		mouseConnection1:disconnect()
	end
	if mouseConnection2 ~= nil then
		mouseConnection2:disconnect()
	end
	if moveConnection ~= nil then
		moveConnection:disconnect()
	end
	if keyConnection ~= nil then
		keyConnection:disconnect()
	end
	mouseDown = false

	toolbar.Visible = false
	paletteFrame.Visible = false
	inPalette = false
	lineBrick = nil
end

tool.Equipped:connect(onEquippedLocal)
tool.Unequipped:connect(onUnequippedLocal)

for n,tool in pairs(toolsMenu:getChildren()) do
	tool.MouseButton1Down:connect(function () toolSelected(tool.Name) end)
end
for n,option in pairs(brushOptions.Circle:getChildren()) do
	option.MouseButton1Down:connect(function () brushSelected(option) end)
end
for n,option in pairs(brushOptions.Square:getChildren()) do
	option.MouseButton1Down:connect(function () brushSelected(option) end)
end
for n,option in pairs(lineOptions:getChildren()) do
	option.MouseButton1Down:connect(function () lineSizeSelected(option) end)
end
