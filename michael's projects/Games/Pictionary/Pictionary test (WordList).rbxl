drawingPlayer = nil
chosenWord = nil

drawingTeam = BrickColor.new(&quot;Black&quot;)
guessingTeam = BrickColor.new(&quot;White&quot;)

voted = {}
voteKeywords = {&quot;giveup&quot;, &quot;give up&quot;, &quot;i give up&quot;, &quot;i give up!&quot;, &quot;giveup/&quot;, &quot;new drawer&quot;, &quot;newdrawer&quot;, &quot;newdrawer/&quot;, &quot;eject/&quot;, &quot;eject&quot;, &quot;eject drawer&quot;, &quot;ejectdrawer&quot;, &quot;jezuigt&quot;} -- Possible commands to vote to kick the drawer from the drawing room

function getVotesNeeded()
	local np = game.Players.NumPlayers

	if np &gt; 10 then return 4 end
	if np &gt; 4   then return 3 end
	return 2
	
end

function getWords(lvl, categories)
	if type(categories) == &quot;string&quot; then
		categories = {categories}
	end
	local matchwords = {}
	for i=1,#words do
		if type(words[i]) == &quot;table&quot; then
			if lvl == nil or lvl == words[i].Value then
				if categories == nil or searchTable(categories, words[i][3]) then
					table.insert(matchwords, words[i])
				end
			end
		elseif type(words[i]) == &quot;string&quot; then
			if lvl == nil or lvl == 1 then
				if categories == nil then
					table.insert(matchwords, words[i])
				end
			end
		end
	end
	return words
end

function choose(t)
	return t[math.random(1, #t)]
end

lastChoice = 0
function chooseWord()
	lastChoice = lastChoice + 1
	if lastChoice &gt; #words then
		shuffleWords()
		lastChoice = 1
	end
	return getWords()[lastChoice]
end

function waitForPlayers()
	local players = game.Players:getChildren()
	newHM(game.Workspace, &quot;Message&quot;, &quot;Waiting&quot;, &quot;Please wait...   There must be at least 2 players to start the game.&quot;)
	while #players &lt; 2 and not (testmode and #players &gt; 0 ) do
		wait(2)
		players = game.Players:getChildren()
	end
	noHM(game.Workspace, &quot;Message&quot;, &quot;Waiting&quot;)
	return choose(players)
end

function waitForChar(player, maxTime)
	for i=1,maxTime * 10 do
		if player.Character ~= nil then return player.Character end
		wait(0.1)
	end
	return nil
end

function giveTool(toolName, player)
	if player == nil then return false end
	local tool = game.Lighting:findFirstChild(toolName)
	if tool == nil then return false end

	tool:clone().Parent = player.Backpack

	return true
end

function removeTool(toolName, player)
	local tool = player.Backpack:findFirstChild(toolName)
	if tool ~= nil then
		tool:remove()
		return true
	else
		if player.Character ~= nil then
			local tool = player.Character:findFirstChild(toolName)
			if tool ~= nil then
				tool:remove()
				return true
			end
		end
	end
	return false
end

function removeToolsFromWorkspace(name)
	for i,c in pairs(game.Workspace:getChildren()) do
		if c:isA(&quot;Tool&quot;) and c.Name == name then
			c:remove()
		end
	end
end

function removeAllTools(name)
	for i,c in pairs(game.Players:getChildren()) do
		removeTool(name, c)
	end

	removeToolsFromWorkspace(name)
end

function removeGui(guiName, player)
	local gui = player.PlayerGui:findFirstChild(guiName)
	if gui ~= nil then
		gui:remove()
		return true
	end
	return false
end

function setDrawingPlayer(player)
	voted = {}
	local drawing = game.Workspace:findFirstChild(&quot;drawing&quot;)
	if drawing ~= nil then drawing:remove() end

	if drawingPlayer ~= nil and drawingPlayer.Parent ~= nil then
		drawingPlayer.TeamColor = guessingTeam

		removeAllTools(&quot;Paintbrush&quot;)
		removeGui(&quot;ColorToolGui&quot;, drawingPlayer)
		--removeTool(&quot;Draw&quot;, drawingPlayer)
		--removeTool(&quot;Eraser&quot;, drawingPlayer)

		noHM(drawingPlayer.PlayerGui, &quot;Hint&quot;, &quot;Drawing&quot;)
		noHM(drawingPlayer.PlayerGui, &quot;Message&quot;, &quot;DrawingM&quot;)
		noHM(drawingPlayer.PlayerGui, &quot;Message&quot;, &quot;drawMsg&quot;)
		noHM(drawingPlayer.PlayerGui, &quot;Hint&quot;, &quot;drawHint&quot;)

		local oldChar = drawingPlayer.Character
		if oldChar ~= nil then
			oldChar:MoveTo(randomViewingPosition())
		end

		drawingPlayer = nil
	end

	if player == nil then
		setDrawingPlayer(waitForPlayers())
		return
	end
	local char = waitForChar(player, 5)
	if char == nil then
		setDrawingPlayer(waitForPlayers())
	end

	char:MoveTo(game.Workspace.DrawingSpawn.Position)
	player.TeamColor = drawingTeam

	newHM(player.PlayerGui, &quot;Message&quot;, &quot;Ready?&quot;, &quot;Get Ready!&quot;)
	wait(2)

	giveTool(&quot;Paintbrush&quot;, player)
	--giveTool(&quot;Draw&quot;, player)
	--giveTool(&quot;Eraser&quot;, player)

	drawingPlayer = player
	chosenWord = chooseWord()

	local wordTxt = chosenWord
	while type(wordTxt) == &quot;table&quot; do
		wordTxt = wordTxt[1]
	end

	newHM(game.Workspace, &quot;Hint&quot;, &quot;Drawing&quot;, player.Name.. &quot; is drawing... Try to guess what he is drawing.&quot;)
	newHM(player.PlayerGui, &quot;Hint&quot;, &quot;Drawing&quot;, &quot;The chosen item for you to draw is &apos;&apos;&quot;.. wordTxt.. &quot;.&apos;&apos;   You may not use the chat during this time.&quot;)
	--newHM(player, &quot;Message&quot;, &quot;DrawingM&quot;, &quot;Draw This: &apos;&apos;&quot;.. wordTxt.. &quot;&apos;&apos;&quot;)

	noHM(player.PlayerGui, &quot;Message&quot;, &quot;Ready?&quot;)
end

function newHM(parent, type, name, text)
	if parent == nil then return nil end

	local newMsg = parent:findFirstChild(name)
	if newMsg ~= nil then
		if newMsg.className == type then
			newMsg.Text = text
		else
			newMsg = Instance.new(type)
			newMsg.Name = name
			newMsg.Text = text
			newMsg.Parent = parent 
		end
	else
		newMsg = Instance.new(type)
		newMsg.Name = name
		newMsg.Text = text
		newMsg.Parent = parent 
	end
	return newMsg
end

function noHM(parent, type, name)
	if parent == nil then return false end

	local msg = parent:findFirstChild(name)
	if msg ~= nil then
		if msg.className == type then
			msg:remove()
			return true
		end
	end
	return false
end

function ejectDrawer()
	script.winner.Value = &quot;&quot;

	local wordTxt = chosenWord
	while type(wordTxt) == &quot;table&quot; do
		wordTxt = wordTxt[1]
	end

	newHM(game.Workspace, &quot;Message&quot;, &quot;Ejected&quot;, tostring(drawingPlayer).. &quot; has been voted from the drawing room! (The chosen word was &quot;.. wordTxt.. &quot;)&quot;)
	wait(2)
	noHM(game.Workspace, &quot;Message&quot;, &quot;Ejected&quot;)
end

function recountVotes()
	for i=1,#voted do
		if game.Players:findFirstChild(voted[i]) == nil then
			table.remove(voted, i)
			recountVotes()
			return true
		end
	end
	return false
end

function voteNewDrawer(voter)
	if drawingPlayer == nil then return end

	recountVotes()
	for i=1,#voted do
		if voted[i] == voter.Name then -- Already Voted
			newHM(voter.PlayerGui, &quot;Message&quot;, &quot;voteM&quot;, &quot;You have already voted!&quot;)
			newHM(voter.PlayerGui, &quot;Hint&quot;, &quot;voteH&quot;, &quot;You have already voted to eject &quot;.. tostring(drawingPlayer).. &quot; player from the drawing room!&quot;)
			wait(3)
			noHM(voter.PlayerGui, &quot;Message&quot;, &quot;voteM&quot;)
			noHM(voter.PlayerGui, &quot;Hint&quot;, &quot;voteH&quot;)
			return
		end
	end

	table.insert(voted, voter.Name)

	if (#voted &gt;= getVotesNeeded()) then
		delay(nil, ejectDrawer)
		return
	end

	newHM(voter.PlayerGui, &quot;Message&quot;, &quot;voteM&quot;, &quot;Your vote has been counted!&quot;)
	newHM(voter.PlayerGui, &quot;Hint&quot;, &quot;voteH&quot;, &quot;Your vote to eject &quot;.. tostring(drawingPlayer).. &quot; from the drawing room has been counted!&quot;)
	wait(3)
	noHM(voter.PlayerGui, &quot;Message&quot;, &quot;voteM&quot;)
	noHM(voter.PlayerGui, &quot;Hint&quot;, &quot;voteH&quot;)
end

function onChatted(msg, recipient, speaker)
	msg = string.lower(msg)
	if speaker == drawingPlayer then
		script.winner.Value = &quot;&quot;
		newHM(game.Workspace, &quot;Message&quot;, &quot;Cheater!&quot;, speaker.Name.. &quot; talked!&quot;)
		wait(2)
		noHM(game.Workspace, &quot;Message&quot;, &quot;Cheater!&quot;)
	else
		if script.winner.Value == &quot; &quot; then -- Debounce
			if type(chosenWord) == &quot;table&quot; then
				if type(chosenWord[1]) == &quot;table&quot; then
					for i=1,#chosenWord[1] do
						if msg == string.lower(chosenWord[1][i]) then
							script.winner.Value = speaker.Name
						end
					end
				elseif type(chosenWord[1]) == &quot;string&quot; then
					if msg == string.lower(chosenWord[1]) then
						script.winner.Value = speaker.Name
					end
				end
			elseif type(chosenWord) == &quot;string&quot; then
				if msg == string.lower(chosenWord) then
					script.winner.Value = speaker.Name
				end
			end
		end

		for i=1,#voteKeywords do
			if voteKeywords[i] == msg then
				voteNewDrawer(speaker)
				return
			end
		end

	end
end

function onPlayerEntered(newPlayer)
	newPlayer.Chatted:connect(function(msg, recipient) onChatted(msg, recipient, newPlayer) end) 
end

game.Players.ChildAdded:connect(onPlayerEntered)


function playerLeave()
	wait()
	if drawingPlayer == nil or drawingPlayer.Parent == nil or game.Players.NumPlayers &lt; 2 then
		script.winner.Value = &quot;&quot;
	end
end

