		<Item class="Script" referent="RBX24">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">Leaderboard</string>
				<ProtectedString name="Source">names = {&quot;Correct Guesses&quot;} -- The Things you want on the Leaderboard

local stats = Instance.new(&quot;IntValue&quot;)
stats.Name = &quot;leaderstats&quot;

for i=1,#names do
	local v = Instance.new(&quot;IntValue&quot;)
	v.Name = names[i]
	v.Value = 0
	v.Parent = stats
end

function newPlayer(player)
	stats:clone().Parent = player
end

game.Players.ChildAdded:connect(newPlayer)</ProtectedString>
