		if p[i].Character ~= nil and p[i].Character:findFirstChild(&quot;Humanoid&quot;) ~= nil then
			p[i].Character.Humanoid.Health = 0
		end
	end
end

maxTime = 120

function timeCheck()
	local time = 0
	local wordTxt = nil
	while time &lt; maxTime do
		wait(1)
		time = time + 1
		if script.winner.Value ~= &quot; &quot; then
			return
		end

		wordTxt = chosenWord
		while type(wordTxt) == &quot;table&quot; do
			wordTxt = wordTxt[1]
		end
		newHM(drawingPlayer.PlayerGui, &quot;Hint&quot;, &quot;Drawing&quot;, &quot;The chosen item for you to draw is &apos;&apos;&quot;.. wordTxt.. &quot;.&apos;&apos;   You may not use the chat during this time. (&quot;.. tostring(maxTime-time).. &quot; seconds left)&quot;)
	end
	script.winner.Value = &quot;&quot;

	newHM(game.Workspace, &quot;Message&quot;, &quot;TimeExpired&quot;, &quot;Time&apos;s Up! (The chosen word was &quot;.. wordTxt.. &quot;)&quot;)
	wait(2)
	noHM(game.Workspace, &quot;Message&quot;, &quot;TimeExpired&quot;)
end

function waitForResult()
	script.winner.Value = &quot; &quot;
	delay(0.1, timeCheck)
	script.winner.Changed:wait()

	if script.winner.Value == &quot;&quot; then
		wait(2.5)
		return &quot;&quot;
	else
		return script.winner.Value
	end
end

shuffleWords()
setDrawingPlayer(waitForPlayers())
while true do
	local winner = waitForResult()
	if winner ~= nil then
		winner = game.Players:findFirstChild(winner)
	end

	if winner ~= nil then
		local wordTxt = chosenWord
		while type(wordTxt) == &quot;table&quot; do
			wordTxt = wordTxt[1]
		end

		newHM(game.Workspace, &quot;Message&quot;, &quot;Winner&quot;, winner.Name.. &quot; has discovered the chosen word. (&quot;.. wordTxt.. &quot;)&quot;)

		local stats = winner:findFirstChild(&quot;leaderstats&quot;)
		if stats ~= nil then
			local points = stats:findFirstChild(&quot;Correct Guesses&quot;)
			if points ~= nil then
				points.Value = points.Value + 1
			end
		end

		wait(2)

		noHM(game.Workspace, &quot;Message&quot;, &quot;Winner&quot;)

		if winner ~= nil and winner.Parent ~= nil then
			setDrawingPlayer(winner)
		else
			setDrawingPlayer()
		end
	else
		setDrawingPlayer()
	end
end
