					<string name="Name">Spray</string>
					<ProtectedString name="Source">local spraying = false
local dmg = script.Parent.dmg:Clone()
function stick(x, y)

	weld = Instance.new(&quot;Weld&quot;) 



	weld.Part0 = x

	weld.Part1 = y



	local HitPos = x.Position



	local CJ = CFrame.new(HitPos) 

	local C0 = x.CFrame:inverse() *CJ 

	local C1 = y.CFrame:inverse() * CJ 



	weld.C0 = C0 

	weld.C1 = C1 

	

	weld.Parent = x

end

function spray(mouse)
local j = Instance.new(&quot;Part&quot;)
j.BrickColor = script.Parent.Part.BrickColor
j.formFactor = 2
j.Shape = &quot;Ball&quot;
j.Size = Vector3.new(1,1,1)
j.Position = mouse.hit.p
j.Parent = script.Parent.Parent
j.CanCollide = false
j.BottomSurface = 0
j.TopSurface = 0
j.Anchored = true
dmg.Parent = j
dmg.Disabled = false
if mouse.Target.Anchored == false then
j.Anchored = false
stick(j,mouse.Target)
end
end

function stopspraying()
spraying = false
end

function startspraying(mouse)
spraying = true
while spraying == true do
wait(.1)
if (mouse.hit.p - script.Parent.Parent.Torso.Position).magnitude &lt; 100 then
spray(mouse)
else
local f = Instance.new(&quot;Message&quot;)
f.Text = &quot;Can&apos;t spray that far.&quot;
f.Parent = game.Players:playerFromCharacter(script.Parent.Parent)
game:getService(&quot;Debris&quot;):addItem(f,4)
spraying = false
end
end
end

function onSelected(mouse)
	mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;
	mouse.KeyDown:connect(changecolor)
	mouse.Button1Down:connect(function() startspraying(mouse) end)
	mouse.Button1Up:connect(function() stopspraying(mouse) end)
end

script.Parent.Equipped:connect(onSelected)</ProtectedString>
