					<string name="Name">PelletScript</string>
					<ProtectedString name="Source">pellet = script.Parent
welded = false
waiting = false

function stick(hit) 
	welded = true
	local weld = Instance.new(&quot;Weld&quot;) 

	weld.Part0 = pellet 
	weld.Part1 = hit 

	local HitPos = pellet.Position + (pellet.CFrame.lookVector * .5) 

	local CJ = CFrame.new(HitPos) 
	local C0 = pellet.CFrame:inverse() *CJ 
	local C1 = hit.CFrame:inverse() * CJ 

	weld.C0 = C0 
	weld.C1 = C1 

	weld.Parent = pellet 
	local character = pellet.creator.Value.Character
	local torso = character:findFirstChild(&quot;Torso&quot;)
	if torso ~= nil then
		prop = Instance.new(&quot;RocketPropulsion&quot;)
		prop.Target = pellet
		prop.Parent = torso
		prop.MaxSpeed = 100
		prop.MaxThrust = 4e+004
		prop:Fire()
		waiting = true
		wait(5)
		waiting = false
		if prop.Parent ~= nil then
			prop:Abort()
			prop:remove()
		end
	end
end

function onTouched(hit)
	if welded == false then 
		stick(hit) 
	else
		local torso = hit.Parent:findFirstChild(&quot;Torso&quot;)
		if torso ~= nil then
			local propulse = torso:findFirstChild(&quot;RocketPropulsion&quot;)
			if propulse ~= nil then 
				propulse:remove()
				waiting = false
			end
		end
	end
end

pellet.Touched:connect(onTouched)

wait(2)
if welded then 
	wait(2)
	while waiting do 
		wait(0.5)
	end
end
pellet:remove()</ProtectedString>
