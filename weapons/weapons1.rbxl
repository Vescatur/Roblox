			<Item class="Script" referent="RBX16">
				<Properties>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">RopeLauncher</string>
					<ProtectedString name="Source">Ropes = 0 -- The current amount of ropes out.
MaxRopes = 2 -- The maximum ropes out for this player at a time.
MaxDistance = 500 -- The maximum distance for a rope to be created.

function onButton1Down(mouse)
	if Ropes &gt;= MaxRopes then 
		printMessage(&quot;You can only have &quot;..MaxRopes..&quot; ropes out at a time. Press &apos;r&apos; to remove them.&quot;, 6)
		return
	end

	local part = Instance.new(&quot;Part&quot;)
	part.Name = &quot;Rope &quot;..game.Players.LocalPlayer.Name..&quot;&quot;
	part.Anchored = true
	part.TopSurface = 0
	part.BottomSurface = 0
	part.formFactor = &quot;Symmetric&quot;
	part.BrickColor = BrickColor.new(&quot;White&quot;)
	
	local player = game.Players.LocalPlayer.Character
	local torso = player:findFirstChild(&quot;Torso&quot;)
	if torso ~= nil then
		local dist = (mouse.Hit.p - torso.Position)
		if dist.magnitude &gt; MaxDistance then return end
		part.Size = Vector3.new(0,0,dist.magnitude)
		part.CFrame = CFrame.new((mouse.Hit.p + torso.Position)/2, mouse.Hit.p)
		part.Velocity = part.CFrame.lookVector * 30
		part.Parent = workspace
		Ropes = Ropes + 1
	end
end

function onKeyDown(key)
	if key == &quot;r&quot; then
		Ropes = 0
		local get = workspace:getChildren()
		for _,child in pairs(get) do
			if child.Name == &quot;Rope &quot;..game.Players.LocalPlayer.Name..&quot;&quot; then
				child:remove()
			end
		end
	end
end

msg = Instance.new(&quot;Hint&quot;)
function printMessage(text, time)
	msg.Parent = game.Players.LocalPlayer
	msg.Text = text
	wait(time)
	msg.Parent = nil
end

function onSelected(mouse)
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	mouse.KeyDown:connect(onKeyDown)
end

script.Parent.Selected:connect(onSelected)
</ProtectedString>
