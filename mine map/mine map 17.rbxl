				if currentSelection == part then
					currentSelection = nil
					clearSelection()
				end
				oldParent = part.Parent
				part:Remove()
				--The lock will expire in 5 seconds, which is good enough since we deleted the part
			end
		end
		cleanOrphanedModel(oldParent)
	end
end

