&#9;ghostEffect.force = Vector3.new(0, recursiveGetMass(char) * equalizingForce * gravity,0)
&#9;ghostEffect.Parent = script.Parent.Head
end
if player:findFirstChild(&quot;Banned&quot;) then
&#9;script.Parent.Humanoid.Health = 0
end
if player:findFirstChild(&quot;Invisibility&quot;) then
