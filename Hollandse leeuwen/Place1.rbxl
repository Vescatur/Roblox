			<Item class="Script" referent="RBX14">
				<Properties>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">Snap Remover</string>
					<ProtectedString name="Source">--&#9;This script will lower the time required to load a map in online mode. Just leave it in the map. -Gamer3D
function breakAnchored(model)
&#9;if (model.className == &quot;Part&quot;) or (model.className == &quot;Seat&quot;) or (model.className == &quot;SpawnLocation&quot;) then
&#9;&#9;if model.Anchored then
&#9;&#9;&#9;model:BreakJoints()
&#9;&#9;end
&#9;else
&#9;&#9;local child = model:GetChildren()
&#9;&#9;for n = 1, #child do
&#9;&#9;&#9;breakAnchored(child[n])
&#9;&#9;end
&#9;end
end

function makeUnanchored(model)
&#9;if (model.className == &quot;Part&quot;) or (model.className == &quot;Seat&quot;) or (model.className == &quot;SpawnLocation&quot;) then
&#9;&#9;if not model.Anchored then
&#9;&#9;&#9;model:MakeJoints()
&#9;&#9;end
&#9;else
&#9;&#9;local child = model:GetChildren()
&#9;&#9;for n = 1, #child do
&#9;&#9;&#9;makeUnanchored(child[n])
&#9;&#9;end
&#9;end
end

wait()

breakAnchored(workspace)
makeUnanchored(workspace)</ProtectedString>
