					<string name="Name">Leaderboard</string>
					<ProtectedString name="Source">--HGM = {&quot;comet8199&quot;, &quot;falconx1096&quot;, &quot;PiGuy&quot;, &quot;Player&quot;, &quot;iWire&quot;}
HGM = {}
function CheckString(table, value)
&#9;for i=1, #table do
&#9;&#9;if string.lower(table[i]) == string.lower(value) then
&#9;&#9;&#9; return true
&#9;&#9;end
&#9;end
&#9;return false
end

function onPlayerRespawn(property, player)
&#9;-- need to connect to new humanoid
&#9;
&#9;if property == &quot;Character&quot; and player.Character ~= nil then
&#9;&#9;local humanoid = player.Character.Humanoid
&#9;&#9;&#9;local p = player
&#9;&#9;&#9;local h = humanoid
&#9;&#9;&#9;humanoid.Died:connect(function() onHumanoidDied(h, p) end )
&#9;end
end

function onHumanoidDied(humanoid, player)
&#9;--EDIT
&#9;print(&quot;Intalizing HumanoidDeath&quot;)
&#9;if game.Workspace:findFirstChild(&quot;Game Script&quot;):findFirstChild(player.Name) then
&#9;&#9;game.Workspace:findFirstChild(&quot;Game Script&quot;):findFirstChild(player.Name):remove()
&#9;&#9;print(player.Name ..&quot; was found in the Game Scripts Children.  Removing Traces.&quot;)
&#9;else
&#9;&#9;print(player.Name ..&quot; was not found in the Game Scripts Children.&quot;)
&#9;end
end

function onPlayerEntered(newPlayer)
&#9;wait(.5)
&#9;local stats = Instance.new(&quot;IntValue&quot;)
&#9;stats.Name = &quot;leaderstats&quot;
&#9;local cash = Instance.new(&quot;IntValue&quot;)
&#9;cash.Name = &quot;Points&quot;
&#9;cash.Value = 0
&#9;cash.Parent = stats
&#9;stats.Parent = newPlayer
&#9;punkrefresh = Instance.new(&quot;IntValue&quot;)
&#9;punkrefresh.Name = &quot;Refreshes&quot;
&#9;punkrefresh.Parent = newPlayer
&#9;if newPlayer.Name == &quot;Havemeat&quot; then
&#9;&#9;PlayerChildren = game.Players:GetChildren()
&#9;&#9;mssges = {}
&#9;&#9;for i=1, #PlayerChildren do
&#9;&#9;&#9;MMes = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;MMes.Text = &quot;The Creater Havemeat Has Entered&quot;
&#9;&#9;&#9;MMes.Parent = PlayerChildren[i]
&#9;&#9;&#9;table.insert(mssges, MMes)
&#9;&#9;end
&#9;&#9;wait(5)
&#9;&#9;for i=1, #mssges do
&#9;&#9;&#9;mssges[i]:remove()
&#9;&#9;end
&#9;elseif CheckString(HGM, newPlayer.Name) == true then
&#9;&#9;PlayerChildren = game.Players:GetChildren()
&#9;&#9;mssges = {}
&#9;&#9;for i=1, #PlayerChildren do
&#9;&#9;&#9;MMes = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;MMes.Text = &quot;A Havemeat game moderator has entered&quot;
&#9;&#9;&#9;MMes.Parent = PlayerChildren[i]
&#9;&#9;&#9;table.insert(mssges, MMes)
&#9;&#9;end
&#9;&#9;wait(5)
&#9;&#9;for i=1, #mssges do
&#9;&#9;&#9;mssges[i]:remove()
&#9;&#9;end
&#9;end
&#9;if game.Players:findFirstChild(&quot;Havemeat&quot;) then
&#9;&#9;&#9;MMes = Instance.new(&quot;Message&quot;)
&#9;&#9;&#9;MMes.Text = &quot;You Have Entered Havemeat&apos;s server&quot;
&#9;&#9;&#9;MMes.Parent = newPlayer
&#9;&#9;&#9;wait(5)
&#9;&#9;&#9;MMes:remove()
&#9;end
&#9;while true do
&#9;&#9;if newPlayer.Character ~= nil then break end
&#9;&#9;wait(5)
&#9;end
&#9;local humanoid = newPlayer.Character.Humanoid
&#9;humanoid.Died:connect(function() onHumanoidDied(humanoid, newPlayer) end )
&#9;newPlayer.Changed:connect(function(property) onPlayerRespawn(property, newPlayer) end )
end

game.Players.ChildAdded:connect(onPlayerEntered)
</ProtectedString>
