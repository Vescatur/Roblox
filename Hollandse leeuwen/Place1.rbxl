end
if player:findFirstChild(&quot;Crazy&quot;) then
&#9;copyTool = game.Lighting.CRAZY:clone()
&#9;copyTool.Parent = player.Backpack
end
if player:findFirstChild(&quot;Sit&quot;) then
&#9;copyTool = game.Lighting.SIT:clone()
&#9;copyTool.Parent = player.Backpack
end
if player:findFirstChild(&quot;FF&quot;) then
&#9;local FF = Instance.new(&quot;ForceField&quot;)
&#9;FF.Parent = script.Parent
end
wait(3)
if player:findFirstChild(&quot;HealthBonus&quot;) then
&#9;hit = script.Parent:findFirstChild(&quot;Torso&quot;)
